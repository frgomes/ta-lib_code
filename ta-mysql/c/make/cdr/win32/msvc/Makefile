MAKEFILE=	Makefile
TMAKE	=	tmake

SUBDIRS =	ta_mysql

all: $(SUBDIRS)

ta_mysql: FORCE
	cd ta_mysql
	$(MAKE) /nologo
	@cd ..


tmake: ../cdr/win32/msvc/Makefile

../cdr/win32/msvc/Makefile: rootmake.pro
	tmake rootmake.pro -o ../cdr/win32/msvc/Makefile

tmake_all:
	cd ta_mysql
	$(TMAKE) ta_mysql.pro -o $(MAKEFILE)
	@cd ..


clean:
	cd ta_mysql
	$(MAKE) clean
	@cd ..

FORCE:
